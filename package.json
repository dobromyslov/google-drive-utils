{
  "name": "google-drive-utils",
  "version": "0.9.3",
  "description": "Google Drive Utils. Get rid of boilerplate code.",
  "license": "MIT",
  "repository": {
    "type": "Github",
    "url": "https://github.com/dobromyslov/google-drive-utils"
  },
  "homepage": "\"https://github.com/dobromyslov/google-utils-utils#readme",
  "bugs": {
    "url": "https://github.com/dobromyslov/google-drive-utils/issues"
  },
  "author": "Viacheslav Dobromyslov <viacheslav@dobromyslov.ru>",
  "keywords": [
    "google drive",
    "typescript",
    "nodejs",
    "utils"
  ],
  "files": [
    "dist"
  ],
  "main": "dist",
  "types": "dist",
  "sideEffects": false,
  "scripts": {
    "test": "xo && tsc --noEmit && nyc jasmine-ts",
    "release": "npm run build && np --no-2fa",
    "build": "del-cli dist && tsc"
  },
  "dependencies": {
    "google-auth-library": "^6.0.6",
    "googleapis": "^59.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jasmine": "^3.5.12",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "del-cli": "^3.0.1",
    "eslint-config-xo": "^0.32.1",
    "eslint-config-xo-typescript": "^0.32.0",
    "jasmine": "^3.6.1",
    "jasmine-ts": "^0.3.0",
    "np": "^6.4.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "xo": "^0.33.0"
  },
  "eslintConfig": {
    "extends": [
      "xo/esnext",
      "xo-typescript",
      "xo-typescript/space"
    ]
  },
  "xo": {
    "extends": [
      "xo-typescript"
    ],
    "extensions": [
      "ts"
    ],
    "ignores": [
      "spec/"
    ],
    "space": true,
    "rules": {
      "@typescript-eslint/indent": "off",
      "import/no-unassigned-import": [
        "error",
        {
          "allow": [
            "reflect-metadata"
          ]
        }
      ]
    }
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcovonly",
      "text",
      "text-summary"
    ]
  }
}
